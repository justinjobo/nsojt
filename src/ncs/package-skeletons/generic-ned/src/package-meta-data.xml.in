<ncs-package xmlns="http://tail-f.com/ns/ncs-packages">
  <name>%NAME%</name>
  <package-version>%VERSION%</package-version>
  <description>Skeleton for a generic NED</description>
  <ncs-min-version>%NCSVER%</ncs-min-version>

  <component>
    <name>MyDevice</name>
    <ned>
      <generic>
        <ned-id xmlns:n="http://%DIR%-id">n:%NAME%-id</ned-id>
        <java-class-name>%JAVAPACKAGE%.%CLASS%</java-class-name>
      </generic>
      <device>
        <vendor>%VENDOR%</vendor>
      </device>
    </ned>
  </component>
</ncs-package>
