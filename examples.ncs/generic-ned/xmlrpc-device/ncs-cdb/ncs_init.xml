<config:config xmlns:config="http://tail-f.com/ns/config/1.0"
               xmlns="http://tail-f.com/ns/ncs">
  <devices>
    <device>
      <name>x1</name>
      <address>127.0.0.1</address>
      <port>8045</port>
      <device-type>
        <generic>
          <ned-id
              xmlns:id="http://example.com/xmlrpcdevice/xmlrpc/id">
          id:genxmlrpc</ned-id>
        </generic>
      </device-type>
      <state>
        <admin-state>unlocked</admin-state>
      </state>
      <source/>
      <authgroup>default</authgroup>
    </device>

    <device>
      <name>x2</name>
      <address>127.0.0.1</address>
      <port>8046</port>
      <device-type>
        <generic>
          <ned-id xmlns:id="http://example.com/xmlrpcdevice/xmlrpc/id">
            id:genxmlrpc
          </ned-id>
        </generic>
      </device-type>
      <state>
        <admin-state>unlocked</admin-state>
      </state>
      <source/>
      <authgroup>default</authgroup>
    </device>
    <device>
      <name>x3</name>
      <address>127.0.0.1</address>
      <port>8047</port>
      <device-type>
        <generic>
          <ned-id xmlns:id="http://example.com/xmlrpcdevice/xmlrpc/id">
            id:genxmlrpc
          </ned-id>
        </generic>
      </device-type>
      <state>
        <admin-state>unlocked</admin-state>
      </state>
      <source/>
      <authgroup>default</authgroup>
    </device>

    <authgroups>
      <group>
        <name>default</name>
        <umap>
          <local-user>admin</local-user>
          <remote-name>admin</remote-name>
          <remote-password>admin</remote-password>
        </umap>
        <umap>
          <local-user>oper</local-user>
          <remote-name>oper</remote-name>
          <remote-password>oper</remote-password>
        </umap>
      </group>
    </authgroups>
  </devices>
</config:config>
