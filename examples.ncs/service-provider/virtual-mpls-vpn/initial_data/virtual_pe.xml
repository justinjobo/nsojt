<config xmlns="http://tail-f.com/ns/config/1.0">

  <onvpe xmlns="http://com/example/l3vpn">ce7</onvpe>
  <onvpe xmlns="http://com/example/l3vpn">ce8</onvpe>

  <resource-pools xmlns="http://tail-f.com/pkg/resource-allocator">
    <id-pool xmlns="http://tail-f.com/pkg/id-allocator">
      <name>virtual_pe</name>
      <range>
        <start>1000</start>
        <end>1200</end>
      </range>
    </id-pool>
    <ip-address-pool xmlns="http://tail-f.com/pkg/ipaddress-allocator">
      <name>p_net</name>
      <subnet>
        <address>10.1.1.0</address>
        <cidrmask>24</cidrmask>
      </subnet>
    </ip-address-pool>
    <ip-address-pool xmlns="http://tail-f.com/pkg/ipaddress-allocator">
      <name>ce_net</name>
      <subnet>
        <address>10.2.2.0</address>
        <cidrmask>24</cidrmask>
      </subnet>
    </ip-address-pool>
  </resource-pools>
</config>
