<config-template xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
    <device>
      <name>{/device}</name>
      <config>
        <sys xmlns="http://example.com/router">
          <interfaces>
            <interface>
              <name>{/if}</name>
              <speed>{/if-speed}</speed>
            </interface>
          </interfaces>
        </sys>
      </config>
    </device>
  </devices>
</config-template>
