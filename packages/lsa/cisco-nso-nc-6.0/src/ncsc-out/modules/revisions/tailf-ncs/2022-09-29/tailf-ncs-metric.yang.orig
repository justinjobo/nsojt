          leaf jsonrpc-total {
            type yang:counter64;
            description
              "Total number of JSONRPC sessions completed.";
          }
          leaf snmp-total {
            type yang:counter64;
            description
              "Total number of SNMP sessions completed.";
          }
          leaf cli-total {
            type yang:counter64;
            description
              "Total number of CLI sessions completed.";
          }
        }
      }
      container counter-rate {
