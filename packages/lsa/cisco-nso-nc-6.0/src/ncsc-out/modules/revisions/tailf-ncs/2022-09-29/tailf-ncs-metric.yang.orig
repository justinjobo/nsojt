submodule tailf-ncs-metric {
  yang-version 1.1;
  belongs-to tailf-ncs {
    prefix ncs;
  }

  import tailf-common {
    prefix tailf;
  }
  import ietf-yang-types {
    prefix yang;
  }
  import tailf-xsd-types {
    prefix xs;
  }
  import tailf-ncs-ned {
    prefix ned;
  }

  organization "Tail-f Systems";
  description
    "This module defines a data model for accessing Cisco NCS metric.

     Copyright 2022 Cisco Systems, Inc.
     All rights reserved.
     Permission is hereby granted to redistribute this file without
     modification.";

  revision 2022-09-29 {
    description
      "Released as part of NCS-6.0.";
  }

  typedef counter-rate-name {
    type enumeration {
      enum 1m { tailf:code-name counter-rate-enum-1m; }
      enum 5m { tailf:code-name counter-rate-enum-5m; }
      enum 15m { tailf:code-name counter-rate-enum-15m; }
    }
  }

  typedef counter-rate-rate {
    type decimal64 {
      fraction-digits 3;
    }
  }

  container metric {
    description
      "Collection of counters, gauges and statistics";
    tailf:info
      "Display counters, gauges and statistics";

    leaf sysadmin-enabled {
      description
        "Whether the metric sysadmin context is enabled or not.";
      type boolean;
      default true;
    }
    leaf developer-enabled {
      tailf:hidden debug;
      description
        "Whether the metric developer context is enabled or not.";
      type boolean;
      default false;
    }
    leaf debug-enabled {
      tailf:hidden debug;
      description
        "Whether the metric debug context is enabled or not.";
      type boolean;
      default false;
    }
    container log {
      description
        "Configures a log that logs counters as they are incremented.
         This will slow down NCS, so be careful when enabling this log.
         This log is not rotated, i.e. use logrotate(8).

         Please note that this means that every counter, including
         potentially sensitive data, is logged.  No filtering is done.";
      leaf enabled {
        type boolean;
        default false;
        description
          "If set to 'true', all counters are logged.";
      }
      leaf filename {
        type string;
        default "./ncs-metric.log";
        description
          "The name of the file where the metric log is written.";
      }
    }
    container sysadmin {
      description
        "Collection of sysadmin oriented counters and gauges.";
      tailf:info
        "Display sysadmin counters and gauges";
      config false;
      tailf:callpoint counter {
        tailf:internal;
      }
      container counter {
        description
          "Collection of sysadmin oriented counters.";
        tailf:info
          "Display sysadmin counters";

        container transaction {
          description
            "Collection of transaction oriented counters.";
          tailf:info
            "Display sysadmin transaction counters";

          leaf commit {
            type yang:counter64;
            description
              "Total number of committed transactions.";
          }
        }

        container cdb {
          description
            "Collection of CDB oriented counters.";
          tailf:info
            "Display sysadmin CDB counters";

          leaf compactions {
            type yang:counter64;
            description
              "Total number of CDB compactions.";
          }
          leaf boot-time {
            type xs:duration;
            description
              "Total amount of time spent in boot.";
          }
          leaf phase0-time {
            type xs:duration;
            description
              "Total amount of time spent in startup phase0.";
          }
          leaf phase1-time {
            type xs:duration;
            description
              "Total amount of time spent in startup phase1.";
          }
          leaf phase2-time {
            type xs:duration;
            description
              "Total amount of time spent in startup phase2.";
          }
        }

        container device {
          description
            "Collection of device oriented counters.";
          tailf:info
            "Display sysadmin device counters";

          leaf connect {
            type yang:counter64;
            description
              "Total number of device connects.";
          }
          leaf connect-failed {
            type yang:counter64;
            description
              "Total number of failed device connects.";
          }
          leaf sync-from {
            type yang:counter64;
            description
              "Total number of sync-from invocations.";
          }
          leaf sync-to {
            type yang:counter64;
            description
              "Total number of sync-to invocations.";
          }
          leaf out-of-sync {
            tailf:code-name 'device-out-of-sync';
            type yang:counter64;
            description
              "Total number of out of sync detections.";
          }
        }

        container session {
          tailf:code-name 'session-info';
          description
            "Collection of session oriented counters.";
          tailf:info
            "Display sysadmin session counters";

          leaf total {
            type yang:counter64;
            description
              "Total number of northbound sessions completed.";
